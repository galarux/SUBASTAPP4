{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node build-azure.cjs",
    "build:azure": "rm -rf node_modules package-lock.json && npm install --platform=linux --arch=x64 --legacy-peer-deps && tsc -b && VITE_LEGACY_PEER_DEPS=true vite build",
    "build:check": "tsc -b --noEmit",
    "build:force": "npm install --platform=linux --arch=x64 && tsc -b && vite build",
    "build:clean": "rm -rf node_modules package-lock.json && npm install --platform=linux --arch=x64 && tsc -b && vite build",
    "build:standard": "tsc -b && vite build",
    "build:vite-azure": "node build-vite-azure.cjs",
    "build:manual": "node build-manual.cjs",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "socket.io-client": "^4.8.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  },
  "overrides": {
    "rollup": {
      "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.9.5"
      }
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": ["linux", "win32", "darwin"],
  "cpu": ["x64", "arm64"]
}
